{
  "name": "@tinloof/sanity-studio",
  "version": "0.4.0",
  "description": "A collection of Sanity studio plugins, fields, and components",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "type": "module",
  "author": "Tinloof",
  "scripts": {
    "dev": "run-s watch",
    "prebuild": "rimraf dist",
    "build": "pkg build --strict && pkg --strict",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg watch --strict"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "src",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "sanityPlugin": {
    "verifyPackage": {
      "tsconfig": false
    }
  },
  "dependencies": {
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/document-internationalization": "^2.1.0",
    "@sanity/icons": "^2.11.4",
    "@sanity/image-url": "^1.0.2",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/presentation": "^1.12.1",
    "@sanity/ui": "^2.0.12",
    "@tanstack/react-virtual": "^3.2.0",
    "@tinloof/sanity-web": "workspace:*",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.6",
    "react-rx": "^2.1.3"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^5.1.4",
    "@sanity/plugin-kit": "^3.1.10",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.73",
    "@types/react-is": "^18.2.4",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.36.0",
    "styled-components": "^6.1.8",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
